<h1>Upload CSV</h1>
<div class="form-group upload-form">
    <%= form_tag admin_import_path, multipart: true do %>
        <%= file_field_tag :file, class: "form_control" %>
        <%= check_box_tag :replace %>
        <%= label_tag :replace, 'Overwrite entire directory with file?', class: 'input-inline' %>
        <div>
            <br>
            <%= submit_tag 'Upload', class: "btn btn-default" %>
        </div>
    <% end %>
</div>
<% if @users %>
    <ul class="users">
      <% @users.each do |user| %>
          <li>
            <%= link_to user.full_name, user %>
          </li>
      <% end %>
    </ul>
<% end %>
<%= @users_string %>